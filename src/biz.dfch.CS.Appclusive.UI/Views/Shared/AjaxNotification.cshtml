@model IEnumerable<biz.dfch.CS.Appclusive.UI.Models.AjaxNotificationViewModel>

@foreach(biz.dfch.CS.Appclusive.UI.Models.AjaxNotificationViewModel note in Model){
    if (string.IsNullOrEmpty(note.ElementId))
    {
        <script type="text/javascript">
            $.notify("@note.Message.Replace("\r\n", " ")", "@note.Level");
        </script>
    }
    else
    {
        <script type="text/javascript">
            $("#@note.ElementId").notify("@note.Message.Replace("\r\n", " ")", "@note.Level");
        </script>
    }
}
