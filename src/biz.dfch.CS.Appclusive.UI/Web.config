  <biz.dfch.CS.Appclusive.UI.EntitySearch>
    <entities>
      <!-- 
          entityname: definition with entityname 'default-values' is used for all not defined entities
          filter:  filter expression -> "substringof('{0}',Name)" -> {0} will be filled by the search term see https://msdn.microsoft.com/en-us/library/hh169248(v=nav.90).aspx
          display: Expression or property name to show as result-Value
          searchkey: will be used as search-term after autocomplete entry is selected
          select:  Select expression to load the necessary properties -> Id,Name
                   entity-key must be present in select-Attribute because of ODATA-services (Id)           
      -->
      <add entityname="default-values" filter="substringof('{0}',Name)" display="{Name}" searchkey="Name" select="Id,Name" />
      <add entityname="AuditTrail" filter="substringof('{0}',EntityType)" display="EntityType" searchkey="EntityType" select="Id,EntityType" />
      <!--<add entityname="Approval" filter="Status eq '{0}'" display="{Status}" searchkey="Status" select="Id,Status" />-->
      <add entityname="CatalogueItem" filter="substringof(tolower('{0}'),tolower(Name))" display="{Name}" searchkey="Name" select="Id,Name" />
      <add entityname="Endpoint" filter="substringof('{0}',tolower(Name))" display="{Name}" searchkey="Name" select="Id,Name" />
      <add entityname="Order" filter="substringof('{0}',Name)" display="{Name}" searchkey="Name" select="Id,Name,Created" />
      <add entityname="User" filter="substringof('{0}',Name)" display="{Name} ({ExternalType} - {ExternalId})" searchkey="Name" select="Id,Name,ExternalType,ExternalId" />
      <!--<add entityname="Order" filter="substringof('{0}',Name)" display="{Name} - {Created}" searchkey="Name" select="Id,Name,Created" />-->
    </entities>
  </biz.dfch.CS.Appclusive.UI.EntitySearch>

